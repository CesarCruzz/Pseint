Main.Js 

class Numeros{ 

    vuelto(){ 

         

    } 

    multiplo(){ 

        let num = document.getElementById("num").value 

        let mul = document.getElementById("multi").value 

        let resp = document.getElementById("resp") 

        //console.log(resp.textContent) 

        num = parseInt(num) 

        mul = parseInt(mul) 

        if (num % mul == 0) { 

          resp.textContent = `${mul} Es multiplo de ${num}`   

        }else{ 

            resp.textContent = `${mul} No es multiplo de ${num}` 

        } 

    } 

    divisores(){ 

        var botonCalcular = document.getElementById("calcular"); 

botonCalcular.addEventListener("click", calcularDivisores); 

function calcularDivisores() { 

  var numero = parseInt(document.getElementById("numero").value); 

  var divisores = []; 

  for (var i = 1; i <= numero; i++) { 

    if (numero % i === 0) { 

      divisores.push(i); 

    } 

  } 

  var resultado = document.getElementById("resp"); 

  resultado.innerHTML = "Divisores: " + divisores.join(", "); 

} 

} 

 
 

    sumaDivisores(){ 

        var botonCalcular = document.getElementById("calcular"); 

        botonCalcular.addEventListener("click", calcularSumaDivisores); 

        function calcularSumaDivisores() { 

          var numero = parseInt(document.getElementById("num").value); 

          var sumaDivisores = 0; 

          for (var i = 1; i <= numero; i++) { 

            if (numero % i === 0) { 

              sumaDivisores += i; 

            } 

          } 

          var resultado = document.getElementById("resp"); 

          resultado.innerHTML = "Suma de los divisores: " + sumaDivisores; 

        } 

    } 

    cantidadDivisores(){ 

        var botonCalcular = document.getElementById("calcular"); 

        botonCalcular.addEventListener("click", calcularCantidadDivisores); 

        function calcularCantidadDivisores() { 

          var numero = parseInt(document.getElementById("numero").value); 

          var cantidadDivisores = 0; 

          for (var i = 1; i <= numero; i++) { 

            if (numero % i === 0) { 

              cantidadDivisores++; 

            } 

          } 

          var resultado = document.getElementById("resp"); 

          resultado.innerHTML = "Cantidad de divisores: " + cantidadDivisores; 

        } 

    } 

} 

const numero = new Numeros(); 

 
 

 

