		12: Escribir("Diciembre"); 
			
	FinSegun 
					
9: 
	definir monto,descuento Como Real; 
	
	Escribir "Ingrese el monto total de la compra"; 
	
	Leer monto; 
	
	Si monto > 1000 Entonces 
		
		descuento<-monto * 0.2; 
		
		monto<-monto - descuento; 
		
	FinSi 
	
	Escribir "El monto a pagar es: ", monto; 
					
10: 
	definir num1,num2 Como Entero; 
	
	definir operador Como Real; 
	
	Escribir "Ingrese el primer n�mero:"; 
	
	Leer num1; 
	
	Escribir "Ingrese el segundo n�mero:"; 
	
	Leer num2; 
	
	Escribir "Ingrese el operador (+,-,*,/,mod,div):"; 
	
	Leer operador; 
	
	Segun operador Hacer 
		
		: Escribir "La suma es:", num1 + num2; 
			
		: Escribir "La resta es:", num1 - num2; 
			
			Escribir "La multiplicaci�n es:", num1 * num2; 
			
			Si num2 <> 0 Entonces 
				
				Escribir "La divisi�n es:", num1 / num2 ; 
				
			Sino  
				
				Escribir "No se puede dividir por cero"; 
				
				
				
				Si num2 <> 0 Entonces  
					
					Escribir "El resto es:", num1 mod num2 ; 
					
				Sino  
					
					Escribir "No se puede dividir por cero"; 
					
					Si num2 <> 0 Entonces  
						
						Escribir "La divisi�n entera es:", num1 /num2 ; 
						
					Sino 
						
						Escribir "No se puede dividir por cero"; 
						
					FinSi 
					
				FinSi 
				
			FinSi 
			
	FinSegun 
FinSegun
