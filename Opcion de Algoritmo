					Leer horas; 
					
					
					
					Escribir "Ingrese los minutos:"; 
					
					Leer minutos; 
					
					
					
					segundos <- (horas * 3600) + (minutos * 60); 
					
					
					
					Escribir "El equivalente en segundos es:", segundos;
					
				4: 
					Definir A,B,C como entero ;
					
					Escribir "Ingrese el primer n�mero"; 
					
					Leer A; 
					
					Escribir "Ingrese el segundo n�mero"; 
					
					Leer B; 
					
					Escribir "Ingrese el tercer n�mero"; 
					
					Leer C; 
					
					
					
					Si A=B Y B=C Entonces 
						
						Escribir "Los tres n�meros son iguales"; 
						
					Sino Si A>B Y A>C Entonces 
							
							Escribir "El primer n�mero es el mayor"; 
							
							Si B>C Entonces 
								
								Escribir "El segundo n�mero es el segundo mayor"; 
								
							Sino 
								
								Escribir "El tercer n�mero es el segundo mayor"; 
								
							FinSi 
							
						FinSi 
						
					FinSi 
					
					
					
					Si B>A Y B>C Entonces 
						
						Escribir "El segundo n�mero es el mayor"; 
						
						Si A>C Entonces 
							
							Escribir "El primer n�mero es el segundo mayor"; 
							
						Sino 
							
							Escribir "El tercer n�mero es el segundo mayor"; 
							
						FinSi 
						
					FinSi 
					
					Si C>A Y C>B Entonces 
						
						Escribir "El tercer n�mero es el mayor"; 
						
						Si A>B Entonces 
							
							Escribir "El primer n�mero es el segundo mayor"; 
							
						Sino 
							
							Escribir "El segundo n�mero es el segundo mayor"; 
							
						FinSi 

FinSi 
					
                        5: 
	definir precio,horas,total Como Real; 
	
	Escribir "Escribe el precio por hora"; 
	
	leer precio; 
	
	Escribir "Escribe las horas de estacionamiento"; 
	
	leer horas; 
	
	total<-precio*redon(horas); 
	
	Escribir "El total a pagar por las ",redon(horas), "horas es: $",total; 
					
6: 
	definir imc,peso,kilogramos,categoria Como entero; 
	
	Escribir "Ingrese el imc de la persona"; 
	
	leer imc; 
	
	Escribir "Ingrese el kilogramos de la persona"; 
	
	leer kilogramos; 
	
	Escribir "Ingrese la relacion de una persona con su altura"; 
	
	leer peso; 
	
	kilogramos <- kilogramos * (0.45); 
	
	si peso <= 16 Entonces 
		
		Escribir "Criterio de ingreso"; 
		
	SiNo 
		
		si peso>=16 Entonces 
			
			Escribir "Infra peso"; 
			
		SiNo 
			
			si peso>=18 Entonces 
				
				Escribir "bajo peso"; 
				
			SiNo 
				
				si peso>=24 Entonces 
					
					Escribir "peso normal"; 
					
				SiNo 
					
					si peso>=29 Entonces 
						
						Escribir "sobrepeso"; 
						
					SiNo 
						
						si peso>=34 Entonces 
							
							Escribir "obesidad pre-morbida"; 
							
						SiNo 
							
							si peso>=44 Entonces 
								
								Escribir "obesidad morbida"; 
								
							SiNo 
								
								si peso>=46 Entonces 
									
									Escribir "obesidad hiper-morbida"; 
									
								FinSi 
								
							FinSi 
							
						FinSi 
						
					FinSi 
					
				FinSi 
				
			FinSi 
			
		FinSi 
		
	FinSi 
	
	categoria<-peso*kilogramos*imc; 
	
	Escribir "La categoria de la persona es ",categoria,; 
					
7: 
	Definir dia, mes, dias_transcurridos Como Entero; 
	
    Escribir "Ingrese el d�a:"; 
	
    Leer dia; 
	
    Escribir "Ingrese el mes:"; 
	
    Leer mes; 
	
    dias_transcurridos <- (dia - 1) + (mes - 1) * 31; 
	
    Escribir "Han transcurrido ", dias_transcurridos, " d�as desde el 1 de enero de 2014 hasta el ", dia, " de ", mes; 
					
8: 
	definir num Como Entero;; 
	
