						numero <- num / 10; 
						
						contador <- contador + 1; 
						
					FinMientras 
					
					Escribir "El n�mero ingresado tiene ", contador, " d�gitos."; 
					
				2: 
					definir n,a,b Como Entero; 
					
					Escribir "Dame un numero de tres digitos"; 
					
					leer n; 
					
					a<-trunc(n/100); 
					
