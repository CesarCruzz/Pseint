							cant_divisores <- cant_divisores + 1; 
							
						FinSi 
						
					FinPara 
					
					Escribir "La cantidad de divisores de ", num, " es: ", cant_divisores; 
					
				6: 
					Definir n,suma,divisor Como Entero; 
					
					Escribir Sin Saltar "Ingrese un n�mero:"; 
					
					Leer n; 
					
					suma <- 1; 
					
					Escribir Sin Saltar "1"; 
					
					Para divisor<-2 Hasta n - 1 Con Paso 1 Hacer 
						
						Si n MOD divisor = 0 Entonces 
							
							Escribir Sin Saltar " + ", divisor; 
							
							suma <- suma + divisor; 
							
						FinSi 
						
