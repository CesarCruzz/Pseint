Proceso Ejercicio
	Definir opcion Como Entero;
	Escribir "Menu";
	Escribir "1) Estructuras Selectivas";
	Escribir "2) Estructuras Condicionales";
	Escribir "3) Estructuras aIterativas";
	Escribir "4) Salir";
	Escribir "Eligir opcion ";
	leer opcion;
	segun opcion hacer
		1:
			Escribir "Estructuras Selectivas";
			definir num Como Entero;
			Escribir "Ingrese ejercicio";
			Escribir "Ejercicio 1";
			Escribir "Ejercicio 2";
			Escribir "Ejercicio 3";
			Escribir "Ejercicio 4";
			Escribir "Ejercicio 5";
