				1:
					Definir aaaa Como Entero; 
					
					Definir mes Como Entero; 
					
					Definir dia Como Entero; 
					
					
					
					Escribir "Ingrese la fecha en formato AAAA/MM/DD:"; 
					
					Leer aaaa; 
					
					Leer mes; 
					
					Leer dia; 
					
					Si (aaaa MOD 400 = 0) O ((aaaa MOD 4 = 0) Y (aaaa MOD 100 <> 0)) Entonces 
						
						Escribir "El a�o ", aaaa, " es bisiesto."; 
						
					Sino 
						
						Escribir "El a�o ", aaaa, " no es bisiesto."; 
						
					FinSi 
					
				2: 
					a<-1; 
					
					b<-2; 
					
					c<-3; 
					
					suma<-c*1+b*10+a*100; 
					
					Escribir suma; 
					
					aux<-a; 
					
					a<-c; 
					
					c<-aux; 
					
					suma2<-c*1+b*10+a*100; 
					
					si (a=c) Entonces 
						
						Escribir "Los numeros son capicua"; 
						
					sino  
						
						Escribir "no son capicua"; 
						
					FinSi 
					
				3: 
					Definir horas, minutos, segundos Como Entero; 
					
					
					
