			Escribir "Ejercicio 8";
			Escribir "Ejercicio 9";
			Escribir "Ejercicio 10";
			Escribir "Salir";
			leer num;
			segun num Hacer
				1:
					Escribir "(5 + 3 * 2) + 9 > 3 * 5 * 14 % 3 ";
					
					Escribir "(5 + 6) + 9 > 15 % 3 * 14 ";
					
					Escribir "11 + 9 > 0 ";
					
					Escribir "0 > 0 ";
					
					
					
					Escribir "2 *(4 ? 10 + 8)/2* 36 *(1/2)  ";
					
					Escribir "2 * 1 * 36 * 0.5 ";
					
					
					
					Escribir "260 / 12 + 54 % 3 ? 85 % ";
					
					Escribir "21.6666667 + 0 - 6 = 15.6666667 ";
					
					
					
					Escribir "(48 < 2 * 3) | | (2 * 7 < 12)  ";
					
					Escribir "(48 < 6) | (14 < 12) ";
					
					
					
					Escribir "((8 > 2) | | (932 < 23) ) & 4 == 2 ";
					
					Escribir "((8 > 2) | (932 < 23)) & 4 == 2 es falso. ";
					
				2: 
						
						Definir num1,num2,s,r,m,d,mo Como real; 
						
						Escribir "Ingrese el primer n�mero:"; 
						
						Leer num1; 
						
						Escribir "Ingrese el segundo n�mero:"; 
						
						Leer num2; 
						
						s<-num1+num2; 
						
						r<-num1-num2; 
						
						m<-num1*num2; 
						
						d<-num1/num2; 
						
						mo<-num1%num2; 
						
						
						
						Escribir "La suma es: ", s; 
						
						Escribir "La resta es: ", r; 
						
						Escribir "La multiplicaci�n es: ", m; 
						
						Escribir "La divisi�n es: ", d; 
						
						Escribir "El m�dulo es: ", mo; 
						
					
					3: 
						Definir a,b,c,delta,x1,x2 Como Entero;; 
						
						Escribir "Ingrese el valor de a:"; 
						
						Leer a; 
						
						Escribir "Ingrese el valor de b:"; 
						
						Leer b; 
						
						Escribir "Ingrese el valor de c:"; 
						
						Leer c; 
						
						delta <- b^2 - 4*a*c; 
						
						Si delta < 0 Entonces 
							
							Escribir "La ecuaci�n no tiene soluci�n real"; 
							
						SiNo 
							
							x1 <- -b + sqrtdelta / (2*a); 
							
							x2 <- -b - sqrtdelta / (2*a); 
							
							Si delta = 0 Entonces 
								
								Escribir "La ecuaci�n tiene una soluci�n doble: ", x1; 
								
							SiNo 
								
								Escribir "La ecuaci�n tiene dos soluciones: ", x1, " y ", x2; 
								
							FinSi 
							
						FinSi 
					
					4: 
						Definir CatA, CatB, Hip Como Real; 
						
						Escribir ' ********** '; 
						
						Escribir ' Hipotenusa '; 
						
						Escribir ' ********** '; 
						
						Escribir ' '; 
						
						Escribir 'Cateto A: '; 
						
						Leer CatA; 
						
						Escribir 'Cateto B: '; 
						
						Leer CatB; 
						
						Hip <- Raiz(CatA^2 + CatB^2); 
						
						Escribir 'Hipotenusa: ',Hip; 
					
					5: 
						Escribir "Ingrese un n�mero"; 
						
						Leer num; 
						
						Si num % 2 = 0 Entonces 
							
							Escribir "El n�mero es par"; 
							
						Sino 
							
							Escribir "El n�mero es impar"; 
							
						FinSi 
					
					6: 
						definir n,x,binario Como Real; 
						
						Escribir "Escribe un numero"; 
						
						leer n; 
						
						Escribir "El numero ",n, "convertido a binario es "; 
						
						x<-1; 
						
						binario<-0; 
						
						Mientras n >= 1 Hacer 
							
							si n mod 2=1 Entonces 
								
								binario<-binario+x; 
								
							FinSi 
							
							n<-trunc(n/2); 
							
							x<-x*10; 
							
						FinMientras 
						
						Escribir binario; 
					
					7: 
						Definir numero,unidades_de_mil como entero ;
						
						Escribir "Ingrese un n�mero de cuatro d�gitos:"; 
						
						Leer numero; 
						
						unidades <- numero % 10; 
						
						numero <- numero / 10; 
						
						decenas <- numero % 10; 
						
						numero <- numero / 10; 
						
						centenas <- numero % 10; 
						
						numero <- numero / 10; 
						
						unidades_de_mil <- numero % 10; 
						
						Escribir "Unidades: ", unidades; 
						
						Escribir "Decenas: ", decenas; 
						
						Escribir "Centenas: ", centenas; 
						
						Escribir "Unidades de mil: ", unidades_de_mil; 
					
					8: 
						Definir letra Como Caracter; 
						
						Escribir "Ingresa un caracter "; 
						
						leer letra; 
						
						si letra = "a" o letra ="e" o letra = "i" o letra = "o" o letra = "u" Entonces 
							
							Escribir "El caracter ",letra," es un vocal "; 
							
						SiNo 
							
							si letra = "a" o letra ="e" o letra = "i" o letra = "o" o letra = "u" Entonces 
								
								Escribir "El caracter ",letra, "es una vocal"; 
								
							SiNo 
								
								Escribir "El caracter ",letra, "es una consonante"; 
								
							FinSi 
							
						FinSi 
					
					9: 
						Si "a" > "b" Entonces 
							
							Escribir "El primer caracter es mayor que el segundo"; 
							
						FinSi 
						
						
						
						Si "a" < "b" Entonces 
							
							Escribir "El primer caracter es menor que el segundo"; 
							
						FinSi 
						
						
						
						Si "a" = "b" Entonces 
							
							Escribir "Los dos caracteres son iguales"; 
							
						FinSi 
					
					10: 
						definir frase Como Caracter; 
						
						definir x Como Entero; 
						
						Escribir "ingreds un texto"; 
						
						leer frase; 
						
						Escribir "Converti a: "; 
						
						Escribir "1=Mayusculas"; 
						
						Escribir "2=Minusculas"; 
						
						leer x; 
						
						si x>0 y x<3 Entonces 
							
							si x=1 Entonces 
								
								Escribir "El texto en mayusculas es ",Mayusculas(frase),; 
								
							SiNo 
								
								Escribir "El texto en minusculas es ",Minusculas(frase),; 
								
							FinSi 
							
						SiNo 
							
							Escribir "Ingresa una opcion correcta"; 
							
						FinSi 
				FinSegun
			11: 
			De Otro Modo:
